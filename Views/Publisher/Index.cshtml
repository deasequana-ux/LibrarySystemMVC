@model IEnumerable<LibrarySystemMVC.Models.PublisherModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Publisher Name:
        </th>
        <th>
            Phone Number:
        </th>
        <th>
            Address:
        </th>
        <th>
            Website:
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            Publisher Name:
        </td>
        <td>
            Phone Number:
        </td>
        <td>
            Address:
        </td>
        <td>
            Website:
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
