@model IEnumerable<LibrarySystemMVC.Models.PublisherModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Publisher Name 
        </th>
        <th>
            Phone Number 
        </th>
        <th>
            Address 
        </th>
        <th>
            Website 
        </th>
        <th>
            Publish Year
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(model => item.PublisherName)
            
        </td>
        <td>
            @Html.DisplayFor(model => item.PublisherPhoneNumber)
            
        </td>
        <td>
            @Html.DisplayFor(model => item.PublisherAddress)
            
        </td>
        <td>
            @Html.DisplayFor(model => item.PublisherWebsite)
            
        </td>
        <td>
            @Html.DisplayFor(model => item.PublishYear)
            
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
